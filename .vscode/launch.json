{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": true,
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--src",
                "${workspaceFolder}/playground/vanilla_dense_neuron.py",
                "--dst",
                "${workspaceFolder}/playground/example_dense_neuron.py",
                "--mutation",
                ".05",
                "--new-expr-pull",
                ".1",
                "--min-cloning",
                "14",
                "--random-seed",
                "50",
                "--must-mutate",
            ]
        }
    ]
}